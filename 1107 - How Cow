#include <iostream>
using namespace std;

int main() {

int t;
	cin >> t;
	int x1,y1,x2,y2,x3,y3;
	int m, no = 1;
	
	while(t--){
		cin >> x1 >> y1;
		cin >> x2 >> y2;
		cin >> m;
		cout << "Case " << no << ":" << endl;
		while(m--){
			cin >> x3 >> y3;
			
			if((x3 > x1 && x3 < x2) && (y3 > y1 && y3 < y2))
				cout << "Yes" << endl;
			else
				cout << "No" << endl;
		}
		no++;
	}
	return 0;
}
